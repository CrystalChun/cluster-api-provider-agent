{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "commitMessagePrefix": "NO-ISSUE: ",
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0,

    "enabledManagers": [
        "tekton"
    ],
    
    "tekton": {
      "fileMatch": ["^.tekton/*"]
    },
    "postUpdateOptions": [
      "gomodTidy", "gomodVendor"
    ]
}
